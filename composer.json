{
    "name": "raffaelecarelle/php-error-insight-laravel",
    "description": "Laravel integration for PHP Error Insight (raffaelecarelle/php-error-insight)",
    "type": "library",
    "license": "GPL-3.0-or-later",
    "require": {
        "php": ">=8.2",
        "laravel/framework": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/support": "^9.0|^10.0|^11.0|^12.0",
        "illuminate/contracts": "^9.0|^10.0|^11.0|^12.0",
        "symfony/http-foundation": "^5.4|^6.0|^7.0",
        "raffaelecarelle/php-error-insight": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.4",
        "rector/rector": "^2.2",
        "rector/rector-laravel": "^2.1",
        "friendsofphp/php-cs-fixer": "^3.89",
        "webmozart/assert": "^1.12",
        "phpstan/phpstan": "^2.1",
        "orchestra/testbench": "^10.6"
    },
    "authors": [
        {
            "name": "Raffaele Carelle",
            "email": "raffaele.carelle@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "PhpErrorInsightLaravel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PhpErrorInsightLaravel\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "PhpErrorInsightLaravel\\PhpErrorInsightServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "@clear",
            "@prepare"
        ],
        "clear": "@php vendor/bin/testbench package:purge-skeleton --ansi",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "build": "@php vendor/bin/testbench workbench:build --ansi",
        "serve": [
            "Composer\\Config::disableProcessTimeout",
            "@build",
            "@php vendor/bin/testbench serve --ansi"
        ],
        "lint": [
            "@php vendor/bin/phpstan analyse --verbose --ansi"
        ],
        "test": [
            "@clear",
            "@php vendor/bin/phpunit"
        ]
    }
}
